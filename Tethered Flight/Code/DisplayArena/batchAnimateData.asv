% batchAnimateData

directory = '../Data/Apr21/';
fileStem  = 'RTTF100421-';

fileList = dir([directory,fileStem,'*.mat']);

for file=1:3
    
    load([filename]);
    nSamples = size(data.LAmp,1);
    endTime = floor(nSamples / daqParams.SampleRate);
    
    plotFcn = @dualBox;
    animateData([directory,fileList(file).name],1, endTime, plotFcn);
    
    close all;
    clear global;
end